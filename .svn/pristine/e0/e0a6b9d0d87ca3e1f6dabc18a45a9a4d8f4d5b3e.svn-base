#header{
	margin: auto;
	width: 1280px;
}
#header #clogo{
	width : 100%;
	margin: auto;
	clear: both;
}
#header #clogo > img{
	display: block;
	margin-right: auto;
	margin-left: auto;
	margin-top: 15px;
	margin-bottom: 15px;
}
#header #topmenu{
	float : right;
	margin-right: 20px;
	margin-top: 10px;
}